<%- include("partials/header") -%>
  <script>
    $(document).ready(function() {
      $(".results").hide();
      $("footer").css("margin-top", "5%");
      for (let i = 0; i < 6; i++)
        $(".Type" + String.fromCharCode(i + 65)).hide();
      $(".btn").click(function() {
        $("footer").css("margin-top", "50%");
        var sumArr = [];
        for (let i = 0; i < 6; i++) {
          var sum = 0;
          $("." + String.fromCharCode(i + 65) + " select").each(function(i) {
            sum += parseInt($(this).children("option:selected").val());
          });
          sumArr.push(sum);
        }
        var max = sumArr[0];
        var index = 0;
        for (let i = 1; i < 6; i++)
          if (max < sumArr[i]) {
            max = sumArr[i];
            index = i;
          }
        $(".questions").hide();
        $(".Type" + String.fromCharCode(index + 65)).show();
      });
    });
  </script>
