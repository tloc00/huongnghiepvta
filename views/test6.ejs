<%- include("partials/header") -%>
  <script>
    var sum = [0, 0, 0, 0, 0, 0, 0, 0];
    var character = ["E", "I", "S", "N", "T", "F", "J", "P"];
    var count = 0;
    $(document).ready(function() {
      $("footer").css("margin-top", "5%");
      $(".info").hide();
      $(".btn").click(function() {
        $("footer").css("margin-top", "50%");
        $(".questions").hide();
        $("input").each(function(i) {
          if ($(this).is(":checked"))
            sum[$(this).val()]++;
        });
        var max = sum[0];
        var index = 0;
        for (let i = 1; i < 6; i++)
          if (max < sum[i]) {
            max = sum[i];
            index = i;
          }
        var result = "";
        for (let i = 0; i < 8; i += 2)
          if (sum[i] > sum[i + 1])
            result += character[i];
          else
            result += character[i + 1];
        $("." + result).show();
      });
    });
  </script>
</head>

